{
  "workbench.iconTheme": "material-icon-theme",
  "git.autofetch": true,
  "workbench.list.openMode": "doubleClick",
  "workbench.tree.expandMode": "doubleClick",
  "files.insertFinalNewline": true,
  "editor.renderWhitespace": "boundary",
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "graphql",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "jupyter",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "python",
    "pug",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "swift",
    "text",
    "typescript",
    "typescriptreact",
    "vue",
    "yaml",
    "yml",
    "rb",
    "schema"
  ],
  "editor.tabSize": 2,
  "explorer.autoReveal": false,
  "editor.wordWrap": "on",
  "vsicons.dontShowNewVersionMessage": true,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "editor.detectIndentation": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "window.zoomLevel": 1,
  "vscodeGoogleTranslate.preferredLanguage": "Japanese",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "editor.accessibilitySupport": "off",
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin": "${HOME}/.config/nvim/init.lua",
  "workbench.activityBar.orientation": "vertical",
  "vscode-neovim.compositeKeys": {
    "jj": {
      "command": "vscode-neovim.escape"
    },
  },
  "[ruby]": {
    "editor.defaultFormatter": "Shopify.ruby-lsp"
  },
  "rdbg.rubyVersionManager": "rbenv",
  "cSpell.userWords": [
    "rubocop"
  ],
  "gitlens.views.tags.files.layout": "list",
  "gitlens.views.tags.files.icon": "status",
  "gitlens.views.stashes.files.compact": false,
  "gitlens.views.scm.grouped.views": {
    "commits": true,
    "branches": true,
    "remotes": true,
    "stashes": false,
    "tags": true,
    "worktrees": true,
    "contributors": true,
    "repositories": false,
    "searchAndCompare": false,
    "launchpad": false
  },
  "clock.dateFormat": "hh:MM:ss TT",
  "gitHistory.sideBySide": true,
  "diffEditor.renderSideBySideInlineBreakpoint": 0,
  "cursor.preferNotificationsSameAsChat": true,
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "terminal.integrated.stickyScroll.enabled": true,
  "editor.linkedEditing": true,
  "files.trimTrailingWhitespace": true,
  "editor.guides.bracketPairs": true,
  "workbench.colorTheme": "Default Dark Modern",
  "terminal.integrated.scrollback": 10000,
  "rspec-runner.executePath": "docker compose exec web bundle exec rspec",
}
